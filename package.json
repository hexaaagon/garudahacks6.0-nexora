{
  "name": "claiss",
  "devDependencies": {
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.2.19",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow bun",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db:generate": "turbo run --filter=@nexora/database db:generate",
    "db:migrate": "turbo run --filter=@nexora/database db:migrate",
    "db:drop": "cd packages/database && bunx drizzle-kit drop"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@supabase/ssr": "^0.6.1",
    "better-auth": "^1.3.3"
  }
}
